<Window x:Class="UserInterface.MachineComparer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MachineComparer" Height="300" Width="520.896">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Orientation="Vertical">
            <TextBlock>Oryginalny Automat</TextBlock>
        <Grid Name="OriginalMachine"></Grid>
        </StackPanel>
        <StackPanel Grid.Column="1" Orientation="Vertical">
            <TextBlock>Obliczony Automat</TextBlock>
            <Grid Name="NewMachine"></Grid>
        </StackPanel>
       
    </Grid>
</Window>
